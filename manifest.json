 {
	"manifest_version": 2,
	"name": "ntab ntab",
	"short_name": "ntab*2",
	"description" : "Extension that work with tabs surrounded by new tabs",
	"homepage_url": "https://github.com/no1fushi/ntab-2",
	"version": "1.0.0",
	"author": "no1fushi",
	"permissions": ["tabs", "contextMenus", "bookmarks"],
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"96": "icons/icon96.png",
		"128": "icons/icon128.png"
	},
	"background": {
		"scripts": ["src/background.js"],
		"persistent": false
	}
}
